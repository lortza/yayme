<% content_for(:title, "Accomplishments") %>

<%= render partial: 'shared/filter_form', locals: { destination_url: accomplishments_path } %>

<% @accomplishments.each do |accomplishment| %>
  <%= render partial: 'accomplishment', locals: { accomplishment: accomplishment } %>
<% end %>

<%= will_paginate @accomplishments, :page_links => false %>
